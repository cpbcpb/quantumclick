<div class="card partial" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{this.firstName}} {{this.lastName}}</h5>
    <p class="card-text">{{this.businessName}}</p>
  </div>
  <ul class="list-group list-group-flush flexit" >
        <li class="list-group-item">Phone: {{this.phone}}</li>
        <li class="list-group-item">Website:  <a href="http://{{this.websiteURL}}">{{this.websiteURL}}</a></li>
  </ul>
</div>
